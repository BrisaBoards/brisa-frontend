<!doctype html>
<html lang="en" style="height: 100%">
 <head>
  <title>Brisa Dashboard</title>
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  <meta charset="utf-8">
  <link rel="stylesheet" href="3rdparty/<%= process.env.BS_THEME || 'materia.min.css' %>">
  <link rel="stylesheet" href="brisa.css">
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">
 </head>

 <body style="height: 100%;background-color: transparent; font-family: 'Source Sans Pro'">

  <script src="3rdparty/jquery-3.3.1.min.js"></script>
  <script src="3rdparty/popper.min.js"></script>
  <script src="3rdparty/bootstrap.min.js"></script>

  <script src="3rdparty/Sortable.min.js"></script>
  <% if (!process.env.BACKEND) { %>
  <script src="idb.js?1" type="text/javascript" charset="utf-8"></script>
  <script src="brisa-idb-backend.js?1" type="text/javascript" charset="utf-8"></script>
  <% } %>
  <script src="brisa-api.js?1" type="text/javascript" charset="utf-8"></script>
  <script defer src="fontawesome/js/all.min.js"></script>

  <div id="brisa-container">
    <brisa-main :Brisa="Brisa" :view.sync="Brisa.current_view"></db-main>
  </div>

  </body>
</html>
